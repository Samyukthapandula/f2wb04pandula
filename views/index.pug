extends layout

block content
  h1= title
  p This is #{title} generator.
  button(type="button", class="btn btn-primary",onclick="doBump()") Press me
  div(id="bump") Bump the count
  script.
    var count=0;
    var bump=1;
    function doBump(){
      count=count+bump;
      bump=bump+1;
      document.getElementById("bump").innerHTML="Count "+count+" with Bump "+bump;
    }
  a(href="department") department